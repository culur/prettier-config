{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.1",
    "language": "en",
    "allowCompoundWords": true,
    "words": [
        // alphabetical order
        "culur",
        "czrc",
        "gqlconfig",
        "prettierrc",
        "releaserc",
        "stylelintrc"
    ],
    "ignoreWords": [
        // alphabetical order
        "Phạm",
        "Hồng",
        "Phúc"
    ],
    "ignorePaths": [
        "node_modules/**",
        "package.json",
        "package-lock.json",
        "yarn.lock",

        "**/*.log"
    ]
}
