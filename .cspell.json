{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.1",
    "language": "en",
    "allowCompoundWords": true,
    "dictionaries": ["vi-vn"],
    "import": ["@cspell/dict-vi-vn/cspell-ext.json"],
    "words": [
        // alphabetical order
        "culur",
        "czrc",
        "prettierrc",
        "releaserc"
    ],
    "ignorePaths": [
        "node_modules/**",
        "package.json",
        "package-lock.json",
        "yarn.lock",

        "**/*.log"
    ]
}
